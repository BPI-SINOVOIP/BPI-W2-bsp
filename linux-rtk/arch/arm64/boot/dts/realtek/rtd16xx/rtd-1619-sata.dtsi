	ahci_sata: sata@9803F000 {
		clocks = <&clk_en_4 CLK_EN_SATA_MAC_SYSH>;
		resets = <&rst4 RSTN_SATA_MAC_COM>;
		sata-port@0 {
			reg = <0>;
	/*		phys = <&sata_phy 0>;*/
			resets = <&rst4 RSTN_SATA_MAC_P0>;
			gpios = <&rtk_iso_gpio 65 1 1>;
		};
		sata-port@1 {
			reg = <1>;
			phys = <&sata_phy 1>;
			resets = <&rst4 RSTN_SATA_MAC_P1>;
			gpios = <&rtk_iso_gpio 67 1 1>;
		};
	};
};
