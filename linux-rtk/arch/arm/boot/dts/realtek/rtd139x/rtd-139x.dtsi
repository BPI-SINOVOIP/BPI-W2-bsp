		#address-cells = <1>;
		#size-cells = <0>;
		clocks = <&iclk_en CLK_EN_I2C1>;
		resets = <&irst IRSTN_I2C_1>;
	};

	i2c_5: i2c@0x9801BB00 {
		compatible = "Realtek,rtk-i2c";
		reg = <0x9801BB00 0x400>;
		interrupt-parent = <&mux_intc>;
		interrupts = <0 14>;
		i2c-num = <5>;
		status = "okay";
		pinctrl-names = "default";
